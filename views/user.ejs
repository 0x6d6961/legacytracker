<!DOCTYPE html>
<html lang="en">
<head>
<meta name="author" content="sui">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="language" content="English">
<meta name="description" content="View Player <%= data.player %>">
<meta name="title" content="Legacy Player | <%= data.player %>">
<link rel="canonical" href="https://legacytracker.xyz/user/<%= data.player %>">
<link rel="icon" href="https://crafatar.com/avatars/<%= data.uuid ? data.uuid : '00000000-0000-0000-0000-000000000000' %>?size=128&overlay">
<meta name="theme-color" content="#131313">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.0/css/font-awesome.min.css" />
<link rel="stylesheet" href="../assets/style.css">
<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.1/dist/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
<script src="https://cdn.jsdelivr.net/npm/clipboard@2.0.10/dist/clipboard.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.4/moment.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@sweetalert2/theme-dark@5/dark.css" />
<%- include('../partials/head') %>
<title>Legacy Player | <%= data.player %></title>
</head>
<%- include('../partials/header') %>

<div class="mb-3">
<h2><strong><%= data.player %></strong> <%- data.isValid ? '\<span data-toggle="tooltip" data-placement="top" title="This account is verified">\<span class="badge badge-success">&#10003;\</span>\</span>' : '\<span data-toggle="tooltip" data-placement="top" title="This account is not verified">\<span class="badge badge-danger">&#10007;\</span>\</span>' %></h2>
<% if (data.isValid) { %>
    <small><span id="q_uuid"><%= data.uuid %></span>&ensp;<span class="fa fa-clipboard copybtn" data-clipboard-text="<%= data.uuid %>"></span></small>
<% } %>
</p>
</div>

<div class="row">
<div class="col-lg-4 mb-3">
<div class="card bg-dark">

<center>
<img id="playerImg">
<canvas id="player"></canvas>
</center>
</div>



</div>  
<div class="col-lg-8">
    <div class="table-responsive-lg">
<table class="table table-bordered">
<thead>
<tr>
<th class="text-center text-bold"></th>
<th>Server</th>
<th>Cached on</th>
<th>Updated</th>
</tr>
</thead>
<tbody>

<% data.servers.forEach((element, key) => { %>
<tr>
    <td>
        <center><img src="data:image/png;base64,<%= element.server_icon ? element.server_icon : '' %>" alt="<%= data.server %>" width="50px"></center></td>
    <td>
        <h4><a href="../server/<%= element.server_id %> "><%= element.server %> </a></h4></td>
    <td><p class="first"><%= element.date %></p></td>
    <td><p class="last"><%= element.last_date %></p></td>
</tr>
<% }) %>
</tbody>
</table>
</div>
</div>

<script src="../assets/index.js"></script>
<script>
// first and last date parses
let first = $(".first");
let last = $(".last");

$('.first').each(function(index, element) {
$(this).text(moment.unix($(this).text()).format("DD-MM-YYYY")).show();
});

$('.last').each(function(index, element) {

$(this).text(moment.unix($(this).text()).format("DD-MM-YYYY")).show();
});

</script>
<script src="../assets/skinview3d.bundle.js"></script>

<script>
showPlayerModel($("#q_uuid").text());
</script>

<%- include('../partials/footer') %>

